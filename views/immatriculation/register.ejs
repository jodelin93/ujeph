<%- include("../includes/head")%>
</head>

<body>
 
    <%- include("../includes/left_menu")%>

    <div class="all-content-wrapper">
        <%- include("../includes/logo_top") %> 
        <div class="header-advance-area">
            <%- include("../includes/menu_top",{user:user}) %> 
            <!-- Mobile Menu start -->
           <%- include("../includes/mobile_menu") %> 
            <!-- Mobile Menu end -->

            <div class="product-tab-list tab-pane fade active in" id="description">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="review-content-section">
                            <h4 class="text-danger"><%= message %> </h4>
                            <div id="dropzone1" class="pro-ad">
                                <form action="/immatriculation/register"  id="demo1-upload" method="POST">
                                    <div class="row" style="margin: auto;">
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                            <div class="form-group">
                                                <input name="code_etudiant" type="hidden" class="form-control" value="<%= code_etudiant %>" required   />
                                            </div>
                                            <div class="form-group">
                                                <select name="faculte" class="form-control" required>
                                                    <option value="" selected="" disabled="" >Selectionner Faculté</option>
                                                    <% faculte.forEach(fac =>{ %> 
                                                   
                                                    <option ><%= fac.DISTINCT %> </option>
                                                  
                                                    <% }) %> 
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <select name="niveau" class="form-control" required>
                                                    <option value="" selected="" disabled="" >Selectionner niveau</option>
                                                    <option >I</option>
                                                    <option >II</option>
                                                    <option >III</option>
                                                    <option >IV</option>
                                                    <option >V</option>  
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <select name="vacation" class="form-control" required>
                                                    <option value="" selected="" disabled="" >Selectionner vacation</option>
                                                    <option >A.M</option>
                                                    <option >P.M</option>
                                                    <option >Weekend</option>
                                                     
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <select name="degre" class="form-control" required>
                                                    <option value="" selected="" disabled="" >Selectionner Degre</option>
                                                    <option >License</option>
                                                    <option >Maitrise</option>
                                                    <option >Doctorat</option>
                                                    
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <select name="annee" class="form-control" required>
                                                    <option value="" selected="" disabled="" >Selectionner Année academique</option>
                                                    <option >2018-2019</option>
                                                    <option >2019-2020</option>
                                                    <option >2020-2021</option>
                                                    
                                                </select>
                                            </div>
                                            
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="payment-adress">
                                                <button type="submit" class="btn btn-primary waves-effect waves-light">Enregistrer</button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    
        <%- include("../includes/footer") %> 
    </div>

  <%- include("../includes/scripts") %> 
</body>

</html>