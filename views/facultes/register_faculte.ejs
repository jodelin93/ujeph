<%- include("../includes/head")%>
</head>

<body>
 
    <%- include("../includes/left_menu")%>

    <div class="all-content-wrapper">
        <%- include("../includes/logo_top") %> 
        <div class="header-advance-area">
            <%- include("../includes/menu_top",{user:user}) %> 
            <!-- Mobile Menu start -->
           <%- include("../includes/mobile_menu") %> 
            <!-- Mobile Menu end -->

            <div class="error-pagewrap">
                <div class="error-page-int">
                    <div class="text-center custom-login">
                        <h3>Enregistrement de faculte</h3>
                        <p>veuillez a bien remplir les champs</p>
                    </div>
                    <div class="content-error">
                        <div class="hpanel">
                            <div class="panel-body">
                                <form action="/faculte/postFaculte"  method="POST">
                                    <div class="row">
                                        <div class="form-group col-lg-12">
                                            <label>code Faculté</label>
                                            <input class="form-control" name="code_faculte" type="number" required>
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <label>nom faculte</label>
                                            <input class="form-control" name="nom_faculte" required>
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <label>Note de Passage</label>
                                            <input class="form-control" type="number" name="note_passage" required>
                                        </div>
                                        
                                        <div class="form-group col-lg-12">
                                            <label>Degre Faculté</label>
                                            <select class="form-control" name="degre_faculte" required>
                                                <option>licence</option>
                                                <option>Maitrise</option>
                                                <option>Doctorat</option>
                                                
                                              </select>
                                        </div>
                                      
                                    </div>
                                    <div class="text-center">
                                        <button class="btn btn-success loginbtn">Enregistrer</button>
                                        <input type="reset" class="btn btn-default"/>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                  
                </div>   
            </div>
        </div>
    
        <%- include("../includes/footer") %> 
    </div>

  <%- include("../includes/scripts") %> 
</body>

</html>