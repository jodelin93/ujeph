






<%- include("../includes/head")%>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" href="https://cdn.datatables.net/1.10.23/css/jquery.dataTables.min.css">
<link rel="stylesheet" href="https://cdn.datatables.net/buttons/1.6.5/css/buttons.dataTables.min.css">
<link rel="stylesheet" href="/css/font-awesome.min.css">
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>

</head>
<body>

<table id="example" class="display" style="width:100%" data-show-columns="true">
    <thead>
        <tr>
            <th>Code Paiement</th>
            <th>Code Etudiant</th>
            <th style="display: none;"> Date Paiement</th>
            <th style="display: none;"> Utilisateur</th>
            <th>Montant</th>
            <th>Mode Paiement</th>
            <th>Objet Paiement</th>
            <th>Nom Faculte</th>
            <th>Annee Academique</th>
            <th> Date Paiement</th>
            <th> Utilisateur</th>
            
            
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% paiement.forEach(element =>{ %> 
        <tr>
            <td><%= element.code %></td>
            <td><a href="/etudiant/profil_etudiant/<%= element.code_etudiant %>"><%= element.code_etudiant %></a></td>
            <td class="hidden"><%= element.etudiant.nom_etudiant %></td>
            <td class="hidden"><%= element.etudiant.prenom_etudiant %></td>
            <td>HTG <%= element.montant %></td>
            <td><%= element.mode_paiement %></td>
            <td><%= element.objet_paiement %></td>
            <td><%= element.nom_faculte %></td>
            <td><%= element.annee_academique %></td>
            <td><%= element.date_paiement.toDateString() %></td>
            <td><%= element.utilisateur %></td>
           
            <td><a href="/depense/edit_depense/<%= element.code_depense %>"><i class="fa fa-edit"> Modifier </i></a></td>
        </tr>
        <% }) %>
    </tbody>
    <tfoot>
        <tr>
            <th>Code Paiement</th>
            <th>Code Etudiant</th>
            <th style="display: none;"> Date Paiement</th>
            <th style="display: none;"> Utilisateur</th>
            <th>Montant</th>
            <th>Mode Paiement</th>
            <th>Objet Paiement</th>
            <th>Nom Faculte</th>
            <th>Annee Academique</th>
            <th> Date Paiement</th>
            <th> Utilisateur</th>
            <th>Action</th>
        </tr>
    </tfoot>
</table>
<a href="/index"> <button class="btn btn-primary">Retour Ã  la Page d'Accueil</button></a>


<script>
    $(document).ready(function() {
    var printCounter = 0;
    
    // Append a caption to the table before the DataTables initialisation
    $('#example').append('<caption style="caption-side: bottom">A fictional company\'s staff table.</caption>');
    
    $('#example').DataTable( {
      dom: 'Bfrtip',
      buttons: [
          'copy',
          {
              extend: 'excel',
              messageTop: 'The information in this table is copyright to Sirius Cybernetics Corp.'
          },
          {
              extend: 'pdf',
              messageBottom: null
          },
          {
              extend: 'print',
       
              messageBottom: null
          }
      ]
    } );
    } );
    
    
    </script>
    
    <script src="https://cdn.datatables.net/1.10.23/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.5/js/dataTables.buttons.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.5/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.5/js/buttons.print.min.js"></script>
    
    
    
    </body>
    </html>
    