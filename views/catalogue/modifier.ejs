<%- include("../includes/head")%>
</head>

<body>
 
    <%- include("../includes/left_menu")%>

    <div class="all-content-wrapper">
        <%- include("../includes/logo_top") %> 
        <div class="header-advance-area">
            <%- include("../includes/menu_top",{user:user}) %> 
            <!-- Mobile Menu start -->
           <%- include("../includes/mobile_menu") %> 
            <!-- Mobile Menu end -->

            <div class="product-tab-list tab-pane fade active in" id="description">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="review-content-section">
   
                            <div id="dropzone1" class="pro-ad">
                                <form action="/catalogue/postEditCatalogue"  id="demo1-upload" method="POST">
                                    <div class="row" style="margin: auto;">
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                            <div class="form-group">
                                                <input name="code_cours" type="hidden" class="form-control" value="<%=code_cours%>" required   />
                                            </div>
                                            <div class="form-group">
                                                <input name="code" type="hidden" class="form-control" value="<%=code_catalogue%>" required   />
                                            </div>
                                            <div class="form-group">
                                                <select name="faculte" class="form-control" required>
                                                    <option value="<%=catalogue.faculte.nom_faculte%>" selected="" ><%=catalogue.faculte.nom_faculte%></option>
                                                    
                                                    <% faculte.forEach(fac =>{ %> 
                            
                                                    <option ><%= fac.DISTINCT %> </option>
                                                  
                                                    <% }) %> 
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <select name="semestre" class="form-control" required>
                                                    <option value="<%=catalogue.semestre%>" selected="" ><%=catalogue.semestre%></option>
                                                    <option >I</option>
                                                    <option >II</option>
                                                    <option >III</option>
                                                    <option >IV</option>
                                                    <option >V</option>  
                                                    <option >VI</option>
                                                    <option >VII</option>
                                                    <option >VIII</option>
                                                    <option >XI</option>
                                                    <option >X</option>  
                                                </select>
                                            </div>
                           
                                            <div class="form-group">
                                                <select name="degre" class="form-control" required>
                                                    
                                                    <option  selected>licence</option>
                                                    <option >Maitrise</option>
                                                    <option >Doctorat</option>
                                                    
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <select name="professeur" class="form-control" required>
                                                    <option value="<%=catalogue.professeur.code_professeur%>" selected="" ><%=catalogue.professeur.nom_professeur %> <%= catalogue.professeur.prenom_professeur %> </option>
                                                    <% professeur.forEach(prof =>{ %> 
                                                   
                                                    <option value="<%= prof.code_professeur %>"><%= prof.nom_professeur %> <%= prof.prenom_professeur %> </option>
                                                  
                                                    <% }) %> 
                                                </select>
                                                
                                            </div>
                                            
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="payment-adress">
                                                <button type="submit" class="btn btn-primary waves-effect waves-light">Modifier</button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    
        <%- include("../includes/footer") %> 
    </div>

  <%- include("../includes/scripts") %> 
</body>

</html>